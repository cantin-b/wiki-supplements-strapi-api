{
  "kind": "collectionType",
  "collectionName": "supplements",
  "info": {
    "singularName": "supplement",
    "pluralName": "supplements",
    "displayName": "Supplement",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "supplements"
    },
    "short_description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "full_description": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "benefits": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "recommended_dosage": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "cycling_guidelines": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "potential_side_effects": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "natural_sources": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "scientific_name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "origin_country": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "available_status": {
      "type": "enumeration",
      "enum": [
        "AVAILABLE",
        "HARD_TO_FIND",
        "BANNED"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "is_banned_in": {
      "type": "component",
      "repeatable": true,
      "component": "shared.ban-entry",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "related_by": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::supplement.supplement",
      "inversedBy": "related_supplements"
    },
    "related_supplements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::supplement.supplement",
      "mappedBy": "related_by"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "supplements"
    },
    "affiliate_info": {
      "type": "component",
      "repeatable": true,
      "component": "shared.affiliate-info",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "is_featured": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
